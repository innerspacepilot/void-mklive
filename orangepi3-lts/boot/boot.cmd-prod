setenv kernel_version "6.6.2_1"
setenv bootargs "console=ttyS0,115200 loglevel=7 root=/dev/block/179:2"
load mmc 1 ${kernel_addr_r} vmlinuz-${kernel_version}
load mmc 1 ${fdt_addr_r} dtbs/dtbs-${kernel_version}/allwinner/sun50i-h6-orangepi-3-lts.dtb
load mmc 1 ${ramdisk_addr_r} initramfs-${kernel_version}.uimg
booti ${kernel_addr_r} ${ramdisk_addr_r} ${fdt_addr_r}

